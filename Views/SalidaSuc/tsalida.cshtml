@model IEnumerable<WebApplication2.Models.ViewModels.inventario>

@{
    Layout = null;
}

<table class="table">
    <tr>
        <th>
            saldo salida
        </th>
        <th>
            moneda
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                $@string.Format("{0:#,0.00}", item.Dbl_SaldoSalida)
            </td>

            <td>
                @item.Txt_Moneda
            </td>

        </tr>
    }

</table>
